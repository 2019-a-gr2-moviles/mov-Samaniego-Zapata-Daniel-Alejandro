<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/activity_main"
        tools:context=".MainActivity">
    <Button
            android:text="CATALOGO PRODUCTO"
            android:layout_width="145dp"
            android:layout_height="67dp"
            android:id="@+id/catProducto" android:background="@color/colorPrimary"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="192dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="123dp"
            android:layout_marginStart="123dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="143dp" android:layout_marginRight="143dp"
            app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginBottom="31dp" app:layout_constraintBottom_toTopOf="@+id/producto"/>
    <Button
            android:text="PRODUCTO"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/producto" android:background="@color/colorPrimary"
            android:layout_marginTop="43dp" app:layout_constraintTop_toBottomOf="@+id/catProducto"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="146dp"
            android:layout_marginStart="146dp" android:layout_marginBottom="337dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="177dp" android:layout_marginRight="177dp"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="1.0"/>



</android.support.constraint.ConstraintLayout>